{
    "manifest_version": 2,
    "name": "tetoda_robot_chromeExtension",
    "description": "chromeExtension Test",
    "version": "1.0.0",
    "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "openPopup.html"
    },
    "content_security_policy": "script-src 'self' https://www.youtube.com/iframe_api https://s.ytimg.com/yts/jsbin/www-widgetapi-vflkJY3IF/www-widgetapi.js https://s.ytimg.com/yts/jsbin/www-widgetapi-vflXI6m7a/www-widgetapi.js; object-src 'self'",
    "permissions": [
      "tabs",
      "<all_urls>"
    ]
  }